        "min-length": 2,
        "max-length": 4
    },

    "custom/keyboard": {
	     "format": "ÔÑú",
        "min-length": 3,
        "max-length": 3,
	     "interval": 10,
	     "on-click": "toggle_wlr_keyboard.sh",
    },

    "sway/window": {
        "format": "{title}",
        "min-length": 1,
        "max-length": 80,
    },

    "custom/recorder": {
        "on-click": "screen-record.sh",
        "return-type": "json",
        "interval": 1,
        "exec": "screen-record-check-status.sh",
        "min-length": 4,
        "max-length": 6,
    },

    "custom/screenshot": {
        "format": "ÔÖ≠",
        "on-click": "screenshot_wayland.sh",
        "min-length": 4,
        "max-length": 4,
        "tooltip": false
    },

    "custom/caps_lock": {
        "exec": "led-monitor.sh --caps",
        "interval": 1,
        "return-type": "json",
        "min-length": 2,
        "max-length": 2
    },
    "custom/num_lock": {
        "exec": "led-monitor.sh --num",
        "interval": 1,
        "return-type": "json",
        "min-length": 2,
        "max-length": 2
    },
    "custom/scroll_lock": {
        "exec": "led-monitor.sh --scroll",
        "interval": 1,
        "return-type": "json",
        "min-length": 3,
        "max-length": 3
    },

    "custom/togglesticky": {
        "exec": "keymap-monitor.sh",
        "interval": 1,
        "return-type": "json",
        "on-click": "keymap-toggle.sh",
    }
}
